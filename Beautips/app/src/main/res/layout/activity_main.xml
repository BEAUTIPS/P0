<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">



    <!-- Frame 455 -->

    <!-- Frame 431 -->


    <TextView
        android:id="@+id/age"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/montserrat"
        android:text="#22 years"
        android:textColor="#0B0606"
        app:layout_constraintStart_toEndOf="@+id/size"
        tools:layout_editor_absoluteY="166dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/stylist_image"
        android:layout_width="94dp"
        android:layout_height="94dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="81dp"
        android:src="@drawable/myimage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <View
        android:id="@+id/frame_431"
        android:layout_width="21.97dp"
        android:layout_height="21.77dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="152dp"
        android:background="@drawable/frame_431"
        android:elevation="1dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />


    <ImageView
        android:layout_width="41dp"
        android:layout_height="0dp"
        android:layout_marginBottom="8dp"
        android:elevation="3dp"
        android:src="@drawable/V"
        app:layout_constraintBottom_toBottomOf="@+id/frame_431"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="72dp" />

    <!-- Beautips advisor -->


    <!-- Followrs, LIke, customers-->

    <!-- separation bar-->


    <TextView
        android:id="@+id/stylist_name"
        android:layout_width="132dp"
        android:layout_height="23dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="120dp"
        android:layout_marginTop="95dp"
        android:layout_marginEnd="169dp"
        android:layout_marginBottom="650dp"
        android:fontFamily="@font/montserrat"
        android:gravity="top"
        android:text="Abby shrinsk"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="16"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.277"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/stylist_title"
        android:layout_width="146dp"
        android:layout_height="18dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="23dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="169dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_vertical"
        android:text="beautips_advisor"
        android:textAppearance="@style/beautips_ad"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="@+id/stylist_name"
        app:layout_constraintTop_toBottomOf="@+id/stylist_name"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="259dp"
        android:layout_marginBottom="512dp"
        android:fontFamily="@font/montserrat"
        android:text="Followers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/body_shape"
        app:layout_constraintVertical_bias="0.062" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="57dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="171dp"
        android:layout_marginBottom="496dp"
        android:fontFamily="@font/montserrat"
        android:text="Likes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.111"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/body_shape" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="53dp"
        android:layout_marginBottom="515dp"
        android:fontFamily="@font/montserrat"
        android:text="Customers"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/body_shape"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/line_"
        android:layout_width="1px"
        android:layout_height="50px"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/line"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="214dp" />

    <View
        android:id="@+id/line_6"
        android:layout_width="1px"
        android:layout_height="50px"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/line"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="153dp"
        tools:layout_editor_absoluteY="215dp" />

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tab"
        android:layout_width="219dp"
        android:layout_height="25dp"
        android:layout_marginTop="44dp"
        android:paddingBottom="2dp"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:tabIndicatorColor="#FA7575"
        app:tabTextAppearance="@style/tab_text"
        app:tabTextColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="96dp">

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Posts" />

        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Reviews" />
    </com.google.android.material.tabs.TabLayout>

    <TextView
        android:id="@+id/nums_follows"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="110"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="241dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/num_likes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="110"
        tools:layout_editor_absoluteX="183dp"
        tools:layout_editor_absoluteY="241dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/num_customers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="110"
        tools:layout_editor_absoluteX="291dp"
        tools:layout_editor_absoluteY="241dp"
        tools:ignore="MissingConstraints" />

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="420dp"
        android:layout_height="442dp"
        android:layout_marginTop="320dp"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/body_shape"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat"
        android:text="#Round Shape"
        android:textColor="#050303"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="166dp" />

    <TextView
        android:id="@+id/size"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:fontFamily="@font/montserrat"
        android:text="#M"
        android:textColor="#0C0606"
        app:layout_constraintStart_toEndOf="@+id/body_shape"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="166dp" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button_follow"
        android:layout_width="69dp"
        android:layout_height="30dp"
        android:background="@android:color/white"
        android:backgroundTint="#FA7575"
        android:fontFamily="@font/montserrat"
        android:text="Follow"

        android:textColor="@color/white"
        android:textSize="9sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="81dp" />

    <TextView
        android:id="@+id/textView17num_review"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="128dp"
        android:fontFamily="@font/montserrat"
        android:text="61 Reviews"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="271dp" />

    <RatingBar
        android:id="@+id/ratingBar"
        android:layout_width="248dp"
        android:layout_height="50dp"
        android:progressTint="#FFEC40"
        android:rating="3"

        android:scaleX="0.6"
        android:scaleY="0.6"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="116dp" />


</androidx.constraintlayout.widget.ConstraintLayout>