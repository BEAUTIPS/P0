<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/what_is_your_age_group"
        android:layout_width="300dp"
        android:layout_height="28dp"
        android:fontFamily="@font/montserrat"
        android:gravity="center_horizontal"
        android:text="What's your age group?"
        android:textColor="@color/black"
        android:textSize="20dp"

        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="107dp" />

    <Button
        android:id="@+id/under_18"
        android:layout_width="139dp"
        android:layout_height="36dp"
        android:fontFamily="@font/montserrat"
        android:text="Under 18"
        android:textSize="14dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="196dp"
        android:background="@drawable/btn_shape"
        />

    <Button
        android:id="@+id/age_22"
        android:layout_width="139dp"
        android:layout_height="36dp"
        android:fontFamily="@font/montserrat"
        android:text="18 - 22"
        android:textSize="14dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="250dp"
        android:background="@drawable/btn_shape"
        />

    <Button
        android:id="@+id/age_29"
        android:layout_width="139dp"
        android:layout_height="36dp"
        android:fontFamily="@font/montserrat"
        android:text="22 - 29"
        android:textSize="14dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="304dp"
        android:background="@drawable/btn_shape"
        />

    <Button
        android:id="@+id/age_39"
        android:layout_width="139dp"
        android:layout_height="36dp"
        android:fontFamily="@font/montserrat"
        android:text="30 - 39"
        android:textSize="14dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="358dp"
        android:background="@drawable/btn_shape"
        />

    <Button
        android:id="@+id/age_40"
        android:layout_width="139dp"
        android:layout_height="36dp"
        android:fontFamily="@font/montserrat"
        android:text="40+"
        android:textSize="14dp"
        android:textAllCaps="false"
        android:textColor="@color/black"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="412dp"
        android:background="@drawable/btn_shape"
        />

    <ImageButton
        android:id="@+id/next"
        android:layout_width="124dp"
        android:layout_height="40dp"
        android:background="@drawable/frame_434"
        android:gravity="center_horizontal"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="657dp" />

</androidx.constraintlayout.widget.ConstraintLayout>